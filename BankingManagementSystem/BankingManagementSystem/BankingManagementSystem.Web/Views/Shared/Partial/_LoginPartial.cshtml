@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

<ul class="nav navbar-nav navbar-right">
 @if (SignInManager.IsSignedIn(User))
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="Panel" asp-action="Index" title="">Dashboard</a>
    </li>
     <li class="nav-item">
        <form class="nav-menu form-inline" asp-controller="Account" asp-action="Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
            <button id="button" type="submit" class="nav-link btn btn-link text-dark">Logout</button>
        </form>
    </li>
}
else
{
    <li class="nav-item"><a asp-action="Login" asp-controller="Account" title="">Login</a></li>
    <li class="nav-item">
        <p>
            <a asp-action="Register" asp-controller="Account" class="btn btn-default navbar-btn" title="">Sign Up</a>
        </p>
    </li>
}
</ul>